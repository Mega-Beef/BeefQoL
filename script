-- Scripts:

local function DUFYR_fake_script() -- AddedParts.Folder 
	local script = Instance.new('LocalScript', AddedParts)

	local folder = Instance.new("Folder")
	folder.Name = "BeefsQoL"
	folder.Parent = game.Workspace
end
coroutine.wrap(DUFYR_fake_script)()

