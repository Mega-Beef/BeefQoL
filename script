local function XKCNT_fake_script() -- RetroObbySign.Click 
	local script = Instance.new('LocalScript', RetroObbySign)

	script.Parent.MouseButton1Click:Connect(function()

		local player = game.Players.LocalPlayer
		player.Character.HumanoidRootPart.CFrame = CFrame.new(-16872.8574, -6.10000229, 4774.94336, -1, 0, 0, 0, 1, 0, 0, 0, -1)

	end)
end
coroutine.wrap(XKCNT_fake_script)()
local function BIKME_fake_script() -- RetroObbySign.Render 
	local script = Instance.new('LocalScript', RetroObbySign)

	local RunService = game:GetService("RunService")
	local Players = game.Players.LocalPlayer

	local ScreenGui = game.StarterGui.QoLGui
	local Sign = QoLGui.RetroObbySign

	RunService.Heartbeat:Connect(function()
		local Camera = game.Workspace.Camera

		local part = game.Workspace:WaitForChild("BeefsQoL").RetroSign:WaitForChild("Text")
		local camera = workspace.CurrentCamera
		local worldPoint = part.Position
		local vector, isOnScreen = camera:WorldToScreenPoint(worldPoint)

		local screenPoint = UDim2.new(0, vector.X, 0, vector.Y)
		local depth = vector.Z

		Sign.Size = UDim2.new(0,30,0,30)
		Sign.Position = screenPoint

		if isOnScreen then
			Sign.Visible = true
		else 
			Sign.Visible = false
		end
	end)

end
coroutine.wrap(BIKME_fake_script)()
